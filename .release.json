{
    "branches": ["main", "_staging"],
    "repositoryUrl": "https://github.com/RalphHightower/RalphHightower",
    
    "release": {
        "branches": ["master", "next", "_staging"]
        }
    
    "plugins": [
            "@semantic-release/verifyConditions",
            "@semantic-release/analyzeCommits",
            "@semantic-release/verifyRelease",
            "@semantic-release/generateNotes",
            "@semantic-release/prepare",
            "@semantic-release/publish",
            "@semantic-release/addChannel",
            "@semantic-release/success",
            "@semantic-release/fail"
    ]
} 
